<!DOCTYPE html>
<html>
<head>
  <title>Logician Quiz</title>
  <link href='https://fonts.googleapis.com/css?family=Raleway|Arvo' rel='stylesheet' type='text/css'>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
 <div class="row">
  <div class="col-sm-1"></div>
  <div class="col-sm-10 text-right">
   <%= link_to fa_icon("home", text:"home"), root_path, class:"btn btn-info"%>
   <%= link_to fa_icon("file", text:"quiz"), quiz_path, class:"btn btn-info" %>
   <%= link_to fa_icon("check-circle", text:"result"), result_path, class:"btn btn-info" %>
   <%= link_to fa_icon("comment", text:"thanks"), thanks_path, class:"btn btn-info" %>
    <%= link_to fa_icon("book", text:"admin"), logicians_path, class:"btn btn-warning" %>
  </div>
  <div class="col-sm-1"></div>
 </div>
</div>


<div class="container">
 <div class="row">
  <div class="col-sm-1"></div>
  <div class="col-sm-10">
    <div class="main_section">
      <%= yield %>
    </div>
  </div>
  <div class="col-sm-1"></div>
 </div>
</div>

</body>
</html>
